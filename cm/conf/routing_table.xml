<?xml version="1.0" encoding="utf-8"?>
<ffxml>
    <rule id="actexparse">
        <priority>HIGH</priority>
        <source>/actexparse$</source>
        <destination>
            <url>/themes/restricted/ff/ffField/widgets/actex/actexparse</url>
        </destination>
        <reverse>/actexparse</reverse>
        <index>999</index>
    </rule>
    <rule id="parsedata">
        <priority>HIGH</priority>
        <source>/parsedata$</source>
        <destination>
            <url>/themes/restricted/ff/ffField/widgets/activecomboex/parsedata</url>
        </destination>
        <reverse>/parsedata</reverse>
        <index>999</index>
    </rule>
    <rule id="aparsedata">
        <priority>HIGH</priority>
        <source>/aparsedata$</source>
        <destination>
            <url>/themes/restricted/ff/ffField/widgets/autocomplete/parsedata</url>
        </destination>
        <reverse>/aparsedata</reverse>
        <index>999</index>
    </rule>
    <rule id="aparsedatax">
        <priority>HIGH</priority>
        <source>/aparsedatax$</source>
        <destination>
            <url>/themes/restricted/ff/ffField/widgets/autocompletex/parsedata</url>
        </destination>
        <reverse>/aparsedatax</reverse>
        <index>999</index>
    </rule> 
    <rule id="atparsedata">
        <priority>HIGH</priority>
        <source>/atparsedata$</source>
        <destination>
            <url>/themes/restricted/ff/ffField/widgets/autocompletetoken/parsedata</url>
        </destination>
        <reverse>/atparsedata</reverse>
        <index>999</index>
    </rule> 
	<rule id="dialog">
		<priority>NORMAL</priority>
		<source>/dialog(/[^/]*){0,1}$</source>
		<destination>
			<url>[MAIN_THEME]/ff/dialog</url>
		</destination>
		<index>999</index>
		<reverse>/dialog</reverse>
	</rule>
	<rule>
		<priority>NORMAL</priority>
		<source>^/admin/layout(.*)</source>
		<destination>
			<url>/cm/contents/layout$1</url>
		</destination>
	</rule>
	<rule>
		<priority>NORMAL</priority>
		<source>^/admin/showfiles(.*)</source>
		<destination>
			<url>/cm/contents/showfiles$1</url>
		</destination>
	</rule>
