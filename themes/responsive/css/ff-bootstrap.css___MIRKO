/* 
* Override Skin
*/
.bs-callout {
  border-left: 3px solid #1976d2;
  background: #FFF;
  color: #1e88e5;
  box-shadow: 0 2px 2px #DDD; }
  .-inverse .bs-callout {
    background: #263238;
    color: white;
    box-shadow: 0 2px 2px #DDD; }
.actions a.btn{
    margin-left:6px;
}
