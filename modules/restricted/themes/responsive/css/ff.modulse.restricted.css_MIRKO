.pbar {
  pointer-events: none; }
  .pbar:before {
    content: "";
    height: 2px;
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    z-index: 1100;
    background: #1e88e5; }
  .pbar:after {
    content: "";
    height: 2px;
    border-radius: 100% 25% 25% 100%;
    opacity: 1;
    position: fixed;
    z-index: 1100;
    top: 0;
    animation: pbloader 2s infinite cubic-bezier(0.45, 1.03, 0.32, 0.905);
    background: #FFF;
    box-shadow: 0 0 10px #1976d2; }

@keyframes pbloader {
  0% {
    width: 0%;
    left: -100px;
    opacity: 0; }
  10% {
    opacity: 1; }
  50% {
    width: 20%; }
  80% {
    width: 100%; }
  100% {
    left: 100%;
    width: 100%; } }
